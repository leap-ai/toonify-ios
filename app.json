{
  "expo": {
    "name": "Toonify AI",
    "slug": "toonify",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "toonify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1917"
    },
    "ios": {
      "buildNumber": "1.0.0",
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.leapai.toonify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.751659734273-j9q11n2ihah1cqa7v5nbr6elbckam3g4"
            ]
          }
        ],
        "NSPhotoLibraryUsageDescription": "Toonify needs access to your photo library to upload images for cartoon generation and to let you update your profile picture."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.leapai.toonify"
    },
    "extra": {
      "eas": {
        "projectId": "1a400000-c128-4cde-b7d3-09ebe2e44450"
      }
    },
    "owner": "leapai"
  }
}
